tspan = (0.0, 30.0)

# â•”â•â•¡ 62245c49-c57f-431f-afc3-de73323732d4
prob = ODEProblem(network, x0, tspan, parameters);

# â•”â•â•¡ d18e69fd-280a-4d21-8078-596dae0a5a72
